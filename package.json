  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.5.1",
    "@vercel/ncc": "^0.38.0",
    "jest": "^29.6.2",
    "jest-circus": "^29.6.2",
