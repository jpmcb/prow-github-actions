  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpmcb/prow-github-actions.git"
  },
  "keywords": [
    "actions",
    "prow"
  ],
  "main": "lib/main.js",
  "scripts": {
